{
    "name": "proximax/php-xpx-chain-sdk",
    "description": "Composer Package for PHP, Wrapper to use the ProximaX Sirius-Chain REST API and SDK.",
    "keywords": ["proximax", "cryptocurrency", "api", "sdk", "restful", "proximax-php", "php-proximax"],
    "license": "MIT",
    "authors": [
        {
            "name": "Gregory Saive",
            "email": "greg@evias.be"
        }
    ],
    "repositories": [],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^8.0.1",
        "illuminate/support": "^8.0.1",
        "guzzlehttp/guzzle": "^7.4.4",
        "mdanter/ecc": "^1.0.0",
        "paragonie/sodium_compat": "^1.17.1",
        "christian-riesen/base32": "^1.6.0",
        "kornrunner/keccak": "dev-master",
        "google/flatbuffers": "^1.11",
        "amphp/websocket-client": "^1.0.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.9",
        "phpunit/phpunit": "^9.0.1",
        "phpspec/phpspec": "^7.2.0",
        "evert/phpdoc-md" : "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Proximax\\": "src/",
            "Proximax\\API\\": "src/api/",
            "Catapult\\Buffers\\": "src/buffers/",
            "Google\\FlatBuffers\\": "php"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proximax\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
